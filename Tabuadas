n1 = int(input('Escolha um numero: '))
a = n1 * 1
b = n1 * 2
c = n1 * 3
d = n1 * 4
e = n1 * 5
f = n1 * 6
g = n1 * 7
i = n1 * 8
j = n1 * 9
k = n1 * 10
print('1x{}={}'.format(n1 , a))
print('2x{}={}'.format(n1 , b))
print('3x{}={}'.format(n1 , c))
print('4x{}={}'.format(n1 , d))
print('5x{}={}'.format(n1 , e))
print('6x{}={}'.format(n1 , f))
print('7x{}={}'.format(n1 , g))
print('8x{}={}'.format(n1 , i))
print('9x{}={}'.format(n1 , j))
print('10x{}={}'.format(n1 , k))
